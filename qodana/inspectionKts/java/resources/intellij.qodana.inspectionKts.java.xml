<idea-plugin package="org.jetbrains.qodana.inspectionKts.java">
  <dependencies>
    <module name="intellij.java.dev"/>
    <module name="intellij.qodana.inspectionKts"/>
    <plugin id="org.jetbrains.kotlin"/>
  </dependencies>
  <extensions defaultExtensionNs="com.intellij.dev">
    <psiViewer.apiMethodsProvider implementation="org.jetbrains.qodana.inspectionKts.java.PsiViewerApiMethodProvider"/>
    <psiViewer.psiViewerActionEnabler implementation="org.jetbrains.qodana.inspectionKts.java.InspectionKtsPsiViewerActionEnabler"/>
  </extensions>
  <extensions defaultExtensionNs="org.jetbrains.qodana.inspectionKts">
    <psiViewerSupport implementation="org.jetbrains.qodana.inspectionKts.java.PsiViewerSupportImpl"/>
    <defaultImportProvider implementation="org.jetbrains.qodana.inspectionKts.java.JavaInspectionKtsDefaultImportsProvider"/>
    <templates.provider implementation="org.jetbrains.qodana.inspectionKts.java.JavaInspectionKtsTemplateProvider"/>
    <examples.provider implementation="org.jetbrains.qodana.inspectionKts.java.JavaInspectionKtsExampleProvider"/>
  </extensions>
</idea-plugin>