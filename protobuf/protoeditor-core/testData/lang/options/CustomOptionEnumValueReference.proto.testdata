syntax = "proto3";
import "descriptor.proto";

package foo.bar;

enum MyEnum {
  FOO = 1;
  BAR = 2;
}

message MyType {
  MyEnum enum_option = 1;
}

extend proto2.FieldOptions {
  MyType test_field_option = 2000;
}

message TestMessage {
  int32 xyz = 1 [(test_field_option).enum_option = FO<caret>O];
}
