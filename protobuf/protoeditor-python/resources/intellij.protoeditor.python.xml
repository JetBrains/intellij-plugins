<idea-plugin package="com.intellij.protobuf.python">
  <dependencies>
    <plugin id="com.intellij.modules.python"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Python -> proto goto -->
    <gotoDeclarationHandler
      implementation="com.intellij.protobuf.python.PbPythonGotoDeclarationHandler"
      order="first"/>
  </extensions>
</idea-plugin>
