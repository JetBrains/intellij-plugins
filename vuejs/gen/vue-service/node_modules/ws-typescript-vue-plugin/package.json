{
  "name": "ws-typescript-vue-plugin",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "htmlparser2": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^13.13.10",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "build:clean": "npm run clean && npm run build",
    "build": "npm run build:node-modules && npm run build:plugin",
    "clean": "rm -Rf ../gen/vue-service",
    "build:plugin": "tsc && cp ./package.json ../gen/vue-service/node_modules/ws-typescript-vue-plugin",
    "build:node-modules": "mkdir -p ../gen/vue-service/node_modules && cp -Rf ./node_modules/htmlparser2 ./node_modules/entities ../gen/vue-service/node_modules/"
  }
}
