{
  "framework": "vue",
  "name": "test-lib",
  "version": "2.0.0-rc.24",
  "contributions": {
    "html": {
      "types-syntax": "typescript",
      "attributes": [
        {
          "name": "v-cool",
          "vue-modifiers": [
            {
              "name": "foo",
              "description": "Foo is cool",
              "doc-url": "http://cool.foo"
            },
            {
              "name": "ba###",
              "description": "Bar is pattern with static prefix",
              "doc-url": "http://cool.bar",
              "pattern": "ba[0-9]{3}"
            },
            {
              "name": "id",
              "description": "Id is completly a pattern",
              "pattern": ".*"
            }
          ]
        }
      ]
    }
  }
}
