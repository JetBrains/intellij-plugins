    "vue-clickaway": "^2.2.2",
    "vuejs-datepicker": "^1.5.3"
  },
  "devDependencies": {
    "bundlesize": "^0.17.0",
    "chalk": "^2.4.1",
    "glob": "^7.1.3",
    "node-sass": "^4.9.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
