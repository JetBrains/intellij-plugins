    "WebComponents",
    "jquery",
    "Popper.js",
    "vue-strap",
    "vue-datepicker",
    "CSS",
    "Flexbox",
    "Alert",
    "Breadcrumb",
    "Button",
    "Checkbox",
    "Radio",
    "Card",
    "Carousel",
    "Slider",
    "Collapse",
    "Dropdown",
    "Select",
    "Option",
    "Input",
    "Jumbotron",
    "List",
    "Nav",
    "Modal",
    "Navbar",
    "Pagination",
    "Popover",
    "Progress",
    "Tab",
    "Tag",
    "Tooltip"
  ],
  "bundlesize": [
    {
      "path": "./dist/shards-vue.common.min.js"
    },
    {
      "path": "./dist/shards-vue.esm.min.js"
    },
    {
      "path": "./dist/shards-vue.umd.min.js"
    }
  ],
  "dependencies": {
