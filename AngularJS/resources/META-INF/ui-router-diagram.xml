<idea-plugin>
  <actions>
    <action class="org.angularjs.codeInsight.router.ShowUiRouterStatesNewDiagramAction" id="AngularJS.Show.Ui.Router.States.Diagram.Action">
      <add-to-group group-id="UML.EditorGroup" />
    </action>

  </actions>

  <extensions defaultExtensionNs="com.intellij">
    <projectService serviceImplementation="org.angularjs.codeInsight.router.AngularUiRouterProviderContext"/>
    <diagram.Provider implementation="org.angularjs.codeInsight.router.AngularUiRouterDiagramProvider"/>
    <javascript.module.provider implementation="org.angularjs.codeInsight.attributes.AngularModulesProvider"/>
  </extensions>

  <resource-bundle>messages.AngularJSBundle</resource-bundle>
</idea-plugin>