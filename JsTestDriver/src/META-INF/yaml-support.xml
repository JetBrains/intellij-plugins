<idea-plugin url="http://www.jetbrains.com/idea">
  <extensions defaultExtensionNs="com.intellij">
    <fileTypeFactory implementation="com.google.jstestdriver.idea.config.JstdConfigFileLoader"/>
    <completion.contributor language="yaml" implementationClass="com.google.jstestdriver.idea.config.JstdConfigFileCompletionContributor"/>
    <gotoDeclarationHandler implementation="com.google.jstestdriver.idea.config.JstdConfigFileGotoFileHandler"/>
    <annotator language="yaml" implementationClass="com.google.jstestdriver.idea.config.JstdConfigFileAnnotator"/>
    <configurationProducer implementation="com.google.jstestdriver.idea.execution.JstdRuntimeConfigurationProducer"/>
  </extensions>
</idea-plugin>
