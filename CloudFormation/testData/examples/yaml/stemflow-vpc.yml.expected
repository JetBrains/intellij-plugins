      - Ref: AZCount
      - '5'
  4PlusAZs:
    Fn::Or:
    - Fn::Equals:
      - Ref: AZCount
      - '4'
    - Fn::Equals:
      - Ref: AZCount
      - '5'
  5PlusAZs:
    Fn::Equals:
    - Ref: AZCount
    - '5'
Description: VPC Network
Parameters:
  AZCount:
    Default: '2'
    Description: The number of Availability Zones that the VPC is to span.
    Type: String
  Environment:
    Default: "TEST"
    Description: Which environment type the resources will be deployed in (e.g. PROD,
      POC, DEV).
    Type: String
  VPCName:
    Default: "StemflowVPC"
    Description: Name of the VPC.
    Type: String
  VPCPrefix:
    AllowedPattern: '[0-9]+\..+'
    Default: '10.25'
    Description: VPC Network Prefix (first two octets ONLY).
    Type: String
