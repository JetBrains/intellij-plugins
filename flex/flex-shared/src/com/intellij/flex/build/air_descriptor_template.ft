        <!--<minSize></minSize>-->

        <!-- The window's initial maximum size, specified as a width/height pair in pixels, such as "1600 1200". Optional. -->
        <!--<maxSize></maxSize>-->

        <!-- The aspect ratio of the app ("portrait" or "landscape" or "any"). Optional. Mobile only. Default is the natural orientation of the device -->
        <!--<aspectRatio></aspectRatio>-->

        <!-- Whether the app will begin auto-orienting on launch. Optional. Mobile only. Default false -->
        ${auto_orients_comment_start}<autoOrients>${auto_orients}</autoOrients>${auto_orients_comment_end}

        <!-- Whether the app launches in full screen. Optional. Mobile only. Default false -->
        ${full_screen_comment_start}<fullScreen>${full_screen}</fullScreen>${full_screen_comment_end}

        <!-- The render mode for the app (either auto, cpu, gpu, or direct). Optional. Default auto -->
        <!--<renderMode></renderMode>-->

