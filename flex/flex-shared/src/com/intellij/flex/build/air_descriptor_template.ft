        <!--<softKeyboardBehavior></softKeyboardBehavior>-->

        <!-- Display Resolution for the app (either "standard" or "high"). Optional, OSX-only. Default "standard" -->
        <!-- <requestedDisplayResolution></requestedDisplayResolution> -->
    </initialWindow>

