    <!-- Identifies the ActionScript extensions used by an application. -->
    ${extensions_comment_start}<extensions>
        ${extensions_list}
    </extensions>${extensions_comment_end}

    <!-- Settings for the application's initial window. Required. -->
    <initialWindow>
